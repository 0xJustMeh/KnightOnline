/* SESSION TIMEOUT */
var down = '';
var downServer = '';
var serverTimerStarted = false;
function Minutes(data) {
    for (var i = 0; i < data.length; i++)
    if (data.substring(i, i + 1) == ":")
    break;
    return (data.substring(0, i));
}
function Seconds(data) {
    for (var i = 0; i < data.length; i++)
    if (data.substring(i, i + 1) == ":")
    break;
    return (data.substring(i + 1, data.length));
}
function Countdown() {
    //$('#showTimer').html(minutes + ':' + seconds);
    seconds--;
    if (seconds == -1) { seconds = 59; minutes--; }
    if (minutes == 0 && seconds == 0) {
        var loadSessionTimeout = function(hash){
            $('#sessionTimeoutPopup').focus();
            $('#sessionTimeout', this).ajaxForm();
            $('#continue').click(function() {
                clearTimeout(down);
                $('select').show();
                timeIt('6:00',1);
            });
        };
        $('select').hide();
        $('#sessionTimeoutPopup').jqm({ajax:'/session_timeout.php',onLoad:loadSessionTimeout,overlay:50,toTop:true,modal:true,zIndex:4000}).jqmShow();
    }
    else down = setTimeout("Countdown()", 1000);
}
function timeIt(time,loggedin) {
    if (loggedin == 1) {
        if (!serverTimerStarted) {
            timeItServer('5:00');
            serverTimerStarted = true;
        }
        minutes = 1 * Minutes(time);
        seconds = 0 + Seconds(time);
        Countdown();
    }
}

/*if (document.captureEvents)
    document.captureEvents(Event.MOUSEMOVE | Event.KEYUP);*/
document.onmousemove = alertkey;
document.onkeyup = alertkey;

function alertkey(e) {
    if (!e) {
        if (window.event)
            e = window.event;
        else
            return;
    }
    if (down != '') {
        clearTimeout(down);
        timeIt('6:00', 1);
    }
}

// Server side NetContinuum timer
function CountdownServer() { 
    secServer--;
    if (secServer == -1) { secServer = 59; minServer--; }
    if (minServer == 0 && secServer == 0) {
        $.post("/_lib/marco_polo.php", {
            postVal: 'marco'
        }, function(response){
            clearTimeout(downServer);
            timeItServer('5:00');
        });
    }
    else downServer = setTimeout("CountdownServer()", 1000);
}
function timeItServer(time) {
    minServer = 1 * Minutes(time);
    secServer = 0 + Seconds(time);
    CountdownServer();
}

/* END SESSION TIMEOUT */
